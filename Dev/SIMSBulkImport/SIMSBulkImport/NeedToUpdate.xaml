<Window x:Class="Matt40k.SIMSBulkImport.NeedToUpdate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Please Update!" Height="500" Width="550" Icon="Branding\icon.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Label Name="label" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center" Content="You appear to be running an out-of-date version!" FontSize="18" />
        <WebBrowser Name="webBrowser" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Width="500" Height="400" />
        <Border BorderBrush="Silver" BorderThickness="1" Grid.Row="1" Height="404" HorizontalAlignment="Left" Margin="6,3,0,0" Name="border1" VerticalAlignment="Top" Width="511" />
    </Grid>
</Window>
