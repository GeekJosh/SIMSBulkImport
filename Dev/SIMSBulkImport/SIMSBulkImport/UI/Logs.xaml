<UserControl x:Class="Matt40k.SIMSBulkImport.Logs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro">
    <Grid>
        <RichTextBox Name="logTextBox" Margin="15,15,15,15" ScrollViewer.VerticalScrollBarVisibility="Visible" IsReadOnly="True" />
        <Button Content="Back" HorizontalAlignment="Right" Margin="50,50,50,50" VerticalAlignment="Bottom" Width="100" Height="30" FontSize="14" Click="backClick"/>
        <Button Content="Submit" HorizontalAlignment="Right" Margin="50,50,170,50" VerticalAlignment="Bottom" Width="100" Height="30" FontSize="14" Click="submitClick"/>
    </Grid>
</UserControl>
