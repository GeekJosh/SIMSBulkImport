<UserControl x:Class="Matt40k.SIMSBulkImport.NeedToUpdate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Label Name="label" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center" Content="You appear to be running an out-of-date version!" FontSize="18" />
        <WebBrowser Name="webBrowser" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Width="900" Height="500" />
    </Grid>
</UserControl>
