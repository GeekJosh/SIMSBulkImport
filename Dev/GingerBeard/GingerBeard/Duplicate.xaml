<Window x:Class="GingerBeard.Duplicate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Duplicate" Height="300" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="231*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="25" />
        </Grid.ColumnDefinitions>
        <DataGrid AutoGenerateColumns="False" Grid.Column="1" Grid.Row="1" Name="dataGrid" Width="400" Height="200" VerticalAlignment="Center" HorizontalAlignment="Center" />
    </Grid>
</Window>
